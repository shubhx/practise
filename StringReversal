public class StringReversal {
    public StringReversal() {
        super();
    }
    
    
   static void getRev(String s2){
    String  s1 = "";    
    for(int i = s2.length()-1; i >=0; i--){
         
        
        s1 = s1 + s2.charAt(i);
        }
        System.out.println(s1);
        
        }
    
    public static void main(String Str[]){
        getRev("Shubham");
        }
}
